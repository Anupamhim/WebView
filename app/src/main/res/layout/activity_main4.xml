<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.anupam.webview.Main4Activity"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:textSize="30dp"
            android:textColor="#000000"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <EditText
            android:id="@+id/editText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:textSize="30dp"
            android:textColor="#d10808"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/bac"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="AC"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/mPlus"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="M+"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/mMinus"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="M-"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/memory"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="M"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />




    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/b1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="1"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b2"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="2"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b3"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="3"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />

        <Button
            android:id="@+id/bplus"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="+"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/b4"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="4"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b5"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="5"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b6"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="6"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />

        <Button
            android:id="@+id/bminus"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="-"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/b7"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="7"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b8"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="8"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/b9"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="9"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />

        <Button
            android:id="@+id/bmul"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="*"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/b0"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="0"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/bpoint"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="."
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/bequal"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="="
            android:textSize="28dp"
            android:textColor="#f4efef"
            />

        <Button
            android:id="@+id/bdiv"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="/"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/dataBase"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="DB"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/dataClear"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="DC"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
        <Button
            android:id="@+id/bc"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="BC"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />



        <Button
            android:id="@+id/memCl"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="70dp"
            android:layout_height="40dp"
            android:background="#000000"
            android:text="H"
            android:textSize="28dp"
            android:textColor="#f4efef"
            />
    </LinearLayout>

</LinearLayout>
